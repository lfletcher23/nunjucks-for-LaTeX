{
  "name": "nunjucks-for-latex",
  "displayName": "nunjucks-for-LaTeX",
  "description": "Syntax highlighting for a custom Nunjucks syntax designed to be LaTeX compatible",
  "version": "1.0.1",
  "publisher": "lfletcher23",
  "icon": "./images/NJ_Tex_Image_128.png",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lfletcher23/nunjucks-for-LaTeX"
  },
  "contributes": {
    "languages": [{
      "id": "nunjucks",
      "aliases": ["nunjucks"],
      "extensions": [".njk",".nunjucks"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "nunjucks",
      "scopeName": "source.nunjucks",
      "path": "./syntaxes/nunjucks.tmLanguage.json"
    }]
  }
}
